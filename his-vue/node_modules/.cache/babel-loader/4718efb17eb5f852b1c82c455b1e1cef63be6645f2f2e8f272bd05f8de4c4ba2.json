st() {\r\n\r\n              this.$http.get('/api/request/list', {params: this.queryCondition}).then(\r\n                  res => {\r\n\r\n                    console.log(res.data.data.checkRequestVOList)\r\n                      this.checkRequestVOList = res.data.data.checkRequestVOList\r\n                      console.log(this.checkRequestVOList)\r\n                      this.checkRequestVOList.forEach(check => {\r\n                          if (check.status == 1) {\r\n                              check.status = \"未缴费\"\r\n                          } else if (check.status == 2) {\r\n                              check.status = \"已缴费\"\r\n                          } else {\r\n                              check.status = \"已出结果\"\r\n                          }\r\n                      })\r\n\r\n                  }\r\n              )\r\n          },\r\n          execute(checkRequest) {\r\n\r\n              // console.log(checkRequest)\r\n              this.$router.push({path: '/inputResult', query: {checkRequest: checkRequest}})\r\n          },\r\n      },\r\n      mounted() {\r\n        this.getCheckList();\r\n      }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n.add-button{\r\n\tposition: absolute;\r\n\tright: 40px;\r\n}\r\n.delete-button{\r\n\tposition: absolute;\r\n\tright: 160px;\r\n}\r\n</style>\r\n"],"mappings":";;;;AAwCA;EAEAA,IAAA,WAAAA,KAAA;IAEA;MACA;;MAEAC,kBAAA;MACA;MACAC,cAAA;QACAC,MAAA;QACAC,UAAA;MAEA;IACA;EACA;EAEAC,OAAA;IAEA;IACAC,YAAA,WAAAA,aAAA;MAAA,IAAAC,KAAA;MAEA,KAAAC,KAAA,CAAAC,GAAA;QAAAC,MAAA,OAAAR;MAAA,GAAAS,IAAA,CACA,UAAAC,GAAA;QAEAC,OAAA,CAAAC,GAAA,CAAAF,GAAA,CAAAZ,IAAA,CAAAA,IAAA,CAAAC,kBAAA;QACAM,KAAA,CAAAN,kBAAA,GAAAW,GAAA,CAAAZ,IAAA,CAAAA,IAAA,CAAAC,kBAAA;QACAY,OAAA,CAAAC,GAAA,CAAAP,KAAA,CAAAN,kBAAA;QACAM,KAAA,CAAAN,kBAAA,CAAAc,OAAA,WAAAC,KAAA;UACA,IAAAA,KAAA,CAAAb,MAAA;YACAa,KAAA,CAAAb,MAAA;UACA,WAAAa,KAAA,CAAAb,MAAA;YACAa,KAAA,CAAAb,MAAA;UACA;YACAa,KAAA,CAAAb,MAAA;UACA;QACA;MAEA,CACA;IACA;IACAc,OAAA,WAAAA,QAAAC,YAAA;MAEA;MACA,KAAAC,O